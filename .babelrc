{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "ie": "11"
      }
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}